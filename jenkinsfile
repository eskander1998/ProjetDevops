pipeline {

        agent any
        stages {
                stage('check out git'){
                    
                steps{
                        echo 'Pulling...';
                        git branch: 'eskander',
                        url : 'https://github.com/eskander1998/ProjetDevops'
                    }
                }
       stage('Testing maven') {
            steps {
                sh """mvn -version"""
                 
            }
        }
       
        stage('MVN CLEAN') {
            steps {
                sh 'mvn clean'
                 
            }
        }
        stage('MVN COMPILE') {
            steps {
                sh 'mvn compile'
                 
            }
        }
        
		stage('SonarQube analysis 1') {
            steps {
                sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=adminadmin'
            }
        }
       
        
    }
}